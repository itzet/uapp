<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <chequefos>
                <active>1</active>
                <model>Urjakart\BackendPayment\Model\Chequefos</model>
                <order_status>pending</order_status>
                <title>FOS Cheque</title>
                <allowspecific>0</allowspecific>
                <group>offline</group>
            </chequefos>
            <cashfos>
                <active>1</active>
                <model>Urjakart\BackendPayment\Model\Cashfos</model>
                <order_status>pending</order_status>
                <title>FOS Cash</title>
                <allowspecific>0</allowspecific>
                <group>offline</group>
            </cashfos>
            <free>
                <group>offline</group>
            </free>
        </payment>
    </default>
</config>
