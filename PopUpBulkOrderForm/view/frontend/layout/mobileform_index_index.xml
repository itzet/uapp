<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Bulk Orders</title>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Urjakart\PopUpBulkOrderForm\Block\BulkOrderForm" name="cms.bulkorderm" template="Urjakart_PopUpBulkOrderForm::mobilebulkorderform.phtml" cacheable="false" />
        </referenceContainer>
        <referenceContainer name="page.main.title" remove="true" />
        <referenceContainer name="header.container" remove="true" />
        <referenceContainer name="navigation.sections" remove="true" />
        <referenceContainer name="footer-container" remove="true" />

    </body>
</page>