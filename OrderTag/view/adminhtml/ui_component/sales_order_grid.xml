<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="sales_order_columns">
        <column name="tag" class="Urjakart\OrderTag\Ui\Component\Listing\Column\Tag">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="visible" xsi:type="boolean">true</item>
                    <item name="label" xsi:type="string" translate="true">Order Tag</item>
                </item>
            </argument>
        </column>
        <column name="gst" class="Urjakart\OrderTag\Ui\Component\Listing\Column\Gst">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="visible" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">GST(Billing)</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>